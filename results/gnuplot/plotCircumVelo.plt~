#!/usr/bin/gnuplot
# GNUPLOT file generated by <cartRing.h> -- MH[DCML] (2010)
# Circumferential velocities at prescribed nodes
set xlabel "time (s)"
set ylabel "circumferential velocities (m/s)"
set terminal png size 1200, 800

set output './pngFiles/nodalCircumVelo.png'
plot      "./datFiles/node_0.dat" usi 1:3 ti "vt_0" w l ,\
          "./datFiles/node_1.dat" usi 1:3 ti "vt_1" w l 