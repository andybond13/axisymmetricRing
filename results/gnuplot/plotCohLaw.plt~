#!/usr/bin/gnuplot
# GNUPLOT file generated by <cartRing.h> -- MH[DCML] (2010)
# Cohesive Law
set xlabel "crack opening (m)"
set ylabel "traction (Pa)"
set terminal png size 1200, 800

s1(x) =    3.000e+08 * ( 1.0 - x/   6.667e-07);
s(x) = ( x <    6.667e-07 ? s1(x) : 0.0 );

e(x) = x *    2.750e+11 /    6.250e-05;

set output "./pngFiles/CohLaw1.png"
set yrange [:   3.000e+08]
plot s(x) ti 'analytical 41' w l lw 3 , \
    './datFiles/cohLaw.dat' usi 2:3 ti 'computed 41' w lp ,\
    e(x) ti 'Elastic Modulus' w l lw 3 ;

s1(x) =    3.000e+08 * ( 1.0 - x/   6.667e-07);
s(x) = ( x <    6.667e-07 ? s1(x) : 0.0 );

e(x) = x *    2.750e+11 /    6.250e-05;

set output "./pngFiles/CohLaw2.png"
set yrange [:   3.000e+08]
