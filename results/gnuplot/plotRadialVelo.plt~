#!/usr/bin/gnuplot
# GNUPLOT file generated by <cartRing.h> -- MH[DCML] (2010)
# Radial velocities at prescribed nodes
set xlabel "time (s)"
set ylabel "radial velocities (m/s)"
set terminal png size 1200, 800

set output './pngFiles/nodalRadialVelo.png'
plot "./datFiles/node_0.dat" usi 1:2 ti "vr_0" w l,\
     "./datFiles/node_1.dat" usi 1:2 ti "vr_1" w l