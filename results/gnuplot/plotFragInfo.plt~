#!/usr/bin/gnuplot
# GNUPLOT file generated by <cartRing.h> -- MH[DCML] (2010)
# Fragmentation Information
set xlabel "time (s)"
set ylabel "Number of Fragments"
set terminal png size 1200, 800

set output './pngFiles/numFrag.png'
plot './datFiles/fraginfo.dat' usi 1:3 ti 'Sum of Damage' w l,\
     './datFiles/fraginfo.dat' usi 1:2 ti '# Frags' w l

set xlabel "time (s)"
set ylabel "stress (Pa)"
set output './pngFiles/SigL_R_C.png'
 plot './datFiles/fraginfo.dat' usi 1:10 ti 'SigL'w l,\
             './datFiles/fraginfo.dat' usi 1:11 ti 'SigR' w l,\
             './datFiles/fraginfo.dat' usi 1:13 ti 'SigC' w l

set logscale y
set xlabel "time (s)"
set ylabel "_Dt"
set terminal png size 1200, 800 

set output './pngFiles/dt.png'
plot './datFiles/energies.dat' usi 1:10 ti 'Refinement' w l,\
     './datFiles/energies.dat' usi 1:9 ti 'Time Step Flag' w l

