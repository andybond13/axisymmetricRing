#!/usr/bin/gnuplot
# GNUPLOT file generated by <cartRing.h> -- MH[DCML] (2010)
# Stresses at prescribed elements
set xlabel "time (s)"
set ylabel "stress (Pa)"
set terminal png size 1200, 800

set output './pngFiles/elmStress.png'
plot "./datFiles/elem_0.dat" usi 1:2 ti "st_0" w l ,\
     "./datFiles/elem_1.dat" usi 1:2 ti "st_1" w l 